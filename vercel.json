{
    "version": 2,
    "builds": [
      {
        "src": "api/webhook.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/webhook",
        "dest": "/api/webhook"
      }
    ],
    "env": {
      "NODE_ENV": "production"
    },
    "functions": {
      "api/webhook.js": {
        "maxDuration": 10
      }
    }
  }